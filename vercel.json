{
  "version": 2,
  "builds": [
    { 
      "src": "index.js", 
      "use": "@vercel/node" 
    },
    {
      "src": "views/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "controllers/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "middleware/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
